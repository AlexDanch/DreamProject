<component name="CustomLable" extends="Label">

    <interface>
        <field id="fontSize" type="integer" value="14" alias="font.size" />
        <field id="uri" type="string" alias="font.uri" />
    </interface>

    <children>
        <Font id="font" uri="pkg:/fonts/Roboto-Bold.ttf" role="font" size="14" />
    </children>

    <script type="text/brightscript" uri="CustomLabel.brs" />

</component>

